package {
    default_applicable_licenses: ["Android-Apache-2.0"],
}

android_app {
    name: "TestImsMediaHal",
    manifest: "AndroidManifest.xml",
    srcs: [
        "src/**/*.java",
    ],
    libs: [
        "android-support-annotations",
    ],
    static_libs: [
       "android.hardware.radio.ims.media-V1-java",
       "ImsMediaFramework",
       "imsmedia-core",
    ],
    optimize: {
        enabled: false,
    },
    dex_preopt: {
        enabled: false,
    },
    owner: "google",
    platform_apis: true,
    privileged: true,
    certificate: "platform",
}


