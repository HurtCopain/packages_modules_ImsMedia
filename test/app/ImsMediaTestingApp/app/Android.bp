package {
    default_applicable_licenses: ["Android-Apache-2.0"],
}

android_app {
    name: "ImsMediaTestingApp",
    manifest: "AndroidManifest.xml",
    resource_dirs: ["src/main/res"],
    min_sdk_version: "current",
    srcs: [
        "src/**/*.java",
    ],
    libs: [
        "android-support-annotations",
    ],
    static_libs: [
        "com.google.android.material_material",
        "android.telephony.imsmedia",
        "androidx.test.rules",
        "androidx.test.core",
        "androidx.test.ext.junit"
    ],
    required: [
        "privapp_permissions_com.example.imsmediatestingapp",
    ],
    owner: "google",
    platform_apis: true,
    privileged: true,
    certificate: "platform",
    optimize: {
        enabled: true,
    },
}

prebuilt_etc {
    name: "privapp_permissions_com.example.imsmediatestingapp",
    src: "privapp-permissions_com.example.imsmediatestingapp.xml",
    sub_dir: "permissions",
}
